    //if lt IE 9
      script(src='https://oss.maxcdn.com/libs/html5shiv/3.7.0/html5shiv.js')
      script(src='https://oss.maxcdn.com/libs/respond.js/1.3.0/respond.min.js')
    script(src='/resources/assets/js/modernizr.min.js')
  body
    // Navigation Bar
    header#topnav
      .topbar-main
        .container
          a.logo(href='/')
            img(src='/resources/assets/images/logo.png', alt='', height='30')
          // End Logo container
          .menu-extras
            .menu-item
              // Mobile menu toggle
              a.navbar-toggle
                .lines
                  span
                  span
                  span
              // End mobile menu toggle
          // end menu-extras
        // end container
      // end topbar-main
      .navbar-custom
        .container
          #navigation
            // Navigation Menu
            ul.navigation-menu
              li.has-submenu
                a(href='/')
                  i.mdi.mdi-view-dashboard
                  | Download
              li.has-submenu
                a(href='/firebase')
                  i.mdi.mdi-diamond
                  | Comming
    .wrapper
    .container
      // Page-Title
      .row
        .col-sm-12

            h2.page-title Graphics PPT
      .row
        .col-sm-12
          .card-box
            h2.page-title 
              strong 공지가 없습니다.
      .row
        .col-sm-12
          .card-box
            h4.m-t-0.header-title
              b Version Update 계획(PPT)
            ul
                li FileName: [title of game][your student ID] [your name].pptx
                li Email: kiun91@hotmail.com
                li Email Subject : [Graphics] [your_student_ID] [your name]
            h4.m-t-0.header-title
              b 내용
            ul
                li Game Concept
                li Bug List
                li Manual(Screenshot)
                li UI
                li Joint/Effector
            p.text-muted.font-13
              


      // end page title end breadcrumb
      .row
        .col-sm-12
          .card-box
            h4.m-t-0.header-title
              b Download Table
            p.text-muted.font-13
              | Fileclick Download
            table.table.table-hover.m-0
              thead
                tr
                  th(scope='col') #
                  th() Title
                  th() Down
              tbody
                -each row in rows
                    tr
                      td(scope="row")=row.id
                      td=row.title
                      td 
                        a(href='/files/'+row.path).btn.btn-danger Down      
      // end row

    // Footer
    footer.footer.text-right
      .container
        .row
          .col-xs-12.text-center
            |2018
            |Graphics
    // End Footer
